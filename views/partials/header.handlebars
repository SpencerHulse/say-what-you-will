<header>
  <div class="header-wrapper">
    <nav>
      <a href="/">Home</a>
      {{#if loggedIn}}
        <a href="/dashboard">Dashboard</a>
        {{! Directly logs you out }}
        <button id="logout">Logout</button>
      {{else}}
        {{! Takes to the login page }}
        <a href="/login">Login</a>
      {{/if}}
    </nav>
    <div class="page-title-wrapper">
      <h1 class="page-title">
        {{headline}}
      </h1>
    </div>
  </div>
</header>